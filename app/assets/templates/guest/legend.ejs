<div class="legend">
<table>
<thead>
<tr>
<th collspan=2>Секонд</th>
</tr>
</thead>
<tbody>
    <% for(var i in icons){
        var icon = icons[i].image,
            title = icons[i].title,
            role = icons[i].role;
 	%>
<tr data-role="<%= role %>">
<td><img src="<%= icon %>" /><td>
    <td><strong><%= title %></strong></td>
</tr>
   <% } %>
</tbody>
</table>
</div>
