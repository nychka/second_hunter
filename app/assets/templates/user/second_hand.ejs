<div class="second-hand">
    <div id="second-title">
               <span id="star" class="glyphicon glyphicon-star-empty pull-left"></span>
               <span class="text-left pull-left" data-name="title"><%= title %></span>
               <% if(status){ %>
               <span id="status" class="glyphicon glyphicon-ok status-trusted pull-left"></span>
               <% } %>
      </div>
      <div class="clearfix"></div>
     <span class="pull-left text-left second-street"><%= address.street %></span>
 <table class="table">
      <thead>
        <tr>
          <th>Пн</th>
          <th>Вт</th>
          <th>Ср</th>
          <th>Чт</th>
          <th>Пт</th>
          <th>Сб</th>
          <th>Нд</th>
        </tr>
      </thead>
      <tbody>
        <tr>
          <td data-role="price" data-name="monday"><%= price.monday %></td>
          <td data-role="price" data-name="tuesday"><%= price.tuesday %></td>
          <td data-role="price" data-name="wednesday"><%= price.wednesday %></td>
          <td data-role="price" data-name="thursday"><%= price.thursday %></td>
          <td data-role="price" data-name="friday"><%= price.friday %> </td>
          <td data-role="price" data-name="saturday"><%= price.saturday %></td>
          <td data-role="price" data-name="sunday"><%= price.sunday %></td>
        </tr>
      </tbody>
    </table>
</div>
