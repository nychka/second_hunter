    <script type="text/javascript"
            src="https://maps.googleapis.com/maps/api/js?key=AIzaSyDQWuApUanpte7w4Enl4WDKrIQ0m5cYf7E&sensor=false&language=uk">
    </script>
    <script type="text/javascript" src="javascripts/google_map.js"></script>
    <script type="text/javascript" src="javascripts/refresh_day.js"></script>
    <script type="text/javascript" src="javascripts/main.js"></script>
